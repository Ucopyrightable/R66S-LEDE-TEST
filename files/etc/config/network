
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option packet_steering '1'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option delegate '0'

config device 'lan_eth0_dev'
	option name 'eth0'
	option macaddr '92:24:c6:cc:fd:d2'

config interface 'wan'
	option ifname 'eth1'
	option _orig_ifname 'eth1'
	option _orig_bridge 'false'
	option proto 'pppoe'
	option username '591800042300025'
	option password '406556'
	option ipv6 'auto'
	option keepalive '0'
	option delegate '0'

config device 'wan_eth1_dev'
	option name 'eth1'
	option macaddr '92:24:c6:cc:fd:d1'

config interface 'wan6'
	option ifname 'eth1'
	option proto 'dhcpv6'
	option delegate '0'
	option reqaddress 'try'
	option reqprefix 'auto'

config interface 'utun'
	option proto 'none'
	option ifname 'utun'
	option device 'utun'

